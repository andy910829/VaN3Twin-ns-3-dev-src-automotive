  <vType id="Car8" accel="4.0" decel="7.5" emergencyDecel="10.0" minGap="1.0" maxSpeed="15.27" laneChangeModel="LC2013" lcStrategic="-1" lcCooperative="-1" lcSpeedGain="0" lcKeepRight="0"/>
  <vType id="Car9" accel="5.0" decel="7.5" emergencyDecel="10.0" minGap="1.0" maxSpeed="16.66" laneChangeModel="LC2013" lcStrategic="-1" lcCooperative="-1" lcSpeedGain="0" lcKeepRight="0"/>

  <!-- 車輛生成：已按 depart 時間升序排列 -->
  <!-- 背景車輛 -->
  <vehicle id="veh2" type="Car5" depart="2.0" route="2" departLane="0" />
  <vehicle id="veh3" type="Car2" depart="4.0" route="3" departLane="1" />

  <!-- **** 主要攻擊場景開始 **** -->
  <!-- 緊急車輛 1 (訊息發送者) -->
  <vehicle id="veh1" type="Car3" depart="5.0" route="0" departLane="1">
  </vehicle>

  <!-- 惡意攻擊車輛 (干擾者) -->
  <vehicle id="veh4" type="Car4" depart="6.0" route="0" departLane="1" />
  
  <!-- 背景車輛 -->
  <vehicle id="veh5" type="Car6" depart="7.5" route="4" departLane="0" />

  <!-- 受害車輛 1 (將從後方高速接近 veh1 和 veh4) -->
