  <vType id="Car4" accel="6.0" decel="7.5" emergencyDecel="10.0" minGap="1.0" maxSpeed="9.72" laneChangeModel="LC2013" lcStrategic="-1" lcCooperative="-1" lcSpeedGain="0" lcKeepRight="0"/>
  <vType id="Car5" accel="4.0" decel="7.5" emergencyDecel="10.0" minGap="1.0" maxSpeed="11.11" laneChangeModel="LC2013" lcStrategic="-1" lcCooperative="-1" lcSpeedGain="0" lcKeepRight="0"/>
